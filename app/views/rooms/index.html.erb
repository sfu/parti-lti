<%
   @title = 'Your Rooms'
   @secondary_link = link_to('Log Out', destroy_user_session_path, :method => :delete)
%>

<% if @filtered %>
    <p>Currently only showing related rooms. <%= link_to 'Show all rooms', root_path %></p>
<% end %>

<ul>
  <% @rooms.each do |room| %>
      <li><%= link_to room.name, room %></li>
  <% end %>
</ul>
<p>Note: Create a room through the creation of an assignment.</p>
